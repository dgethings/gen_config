set interfaces <%= c.interface_identifier %> framing sdh   
set interfaces <%= c.interface_identifier %> description "<%= c.interface_description %>"
set interfaces <%= c.interface_identifier %> traps

set interfaces <%= c.interface_identifier %> speed oc3
set interfaces <%= c.interface_identifier %> clock internal  

set interfaces <%= c.interface_identifier %> encapsulation frame-relay  
set interfaces <%= c.interface_identifier %> no-keepalive
set interfaces <%= c.interface_identifier %> sonet-options fcs 32
set interfaces <%= c.interface_identifier %> hold-time up 10000 down 2000
set interfaces <%= c.interface_identifier %> sonet-options trigger rfi-p hold-time up 2000 down 2000
set interfaces <%= c.interface_identifier %> sonet-options rfc-2615

set interfaces <%= c.interface_identifier %> sonet-options no-payload-scrambler


set interfaces <%= c.interface_identifier %> unit <%= c.white_sub_interface_id %> point-to-point
set interfaces <%= c.interface_identifier %> unit <%= c.white_sub_interface_id %> description <%= c.white_sub_interface_description %>
set interfaces <%= c.interface_identifier %> unit <%= c.white_sub_interface_id %> dlci <%= c.white_sub_interface_id %> 
set interfaces <%= c.interface_identifier %> unit <%= c.white_sub_interface_id %> inverse-arp
set interfaces <%= c.interface_identifier %> unit <%= c.white_sub_interface_id %> family inet address <%= c.ip_address %>/<%= c.0
set interfaces <%= c.interface_identifier %> unit <%= c.white_sub_interface_id %> family mpls

set interfaces <%= c.interface_identifier %> unit <%= c.coloured_sub_interface_id %> point-to-point
set interfaces <%= c.interface_identifier %> unit <%= c.coloured_sub_interface_id %> description <%= c.red/blue_sub_interface_description %>
set interfaces <%= c.interface_identifier %> unit <%= c.coloured_sub_interface_id %> dlci <%= c.coloured_sub_interface_id %>
<%= c.et interfaces <%= c.interface_identifier %> unit <%= c.coloured_sub_interface_id %> inverse-arp
set interfaces <%= c.interface_identifier %> unit <%= c.coloured_sub_interface_id %> family inet address <%= c.ip_address %>/<%= c.0
set interfaces <%= c.interface_identifier %> unit <%= c.coloured_sub_interface_id %> family mpls

set protocols ospf area <%= c.OSPF_area %> interface <%= c.interface_identifier %>.<%= c.white_sub_interface_id %> metric <%= c.ospf_metric %>
set protocols ospf area <%= c.OSPF_area %> interface <%= c.interface_identifier %>.<%= c.white_sub_interface_id %> hello-interval <%= c.OSPF_hello_timer %>
set protocols ospf area <%= c.OSPF_area %> interface <%= c.interface_identifier %>.<%= c.white_sub_interface_id %> dead-interval <%= c.OSPF_dead_timer %>
set protocols ospf area <%= c.OSPF_area %> interface <%= c.interface_identifier %>.<%= c.white_sub_interface_id %> ldp-synchronization  


set protocols ospf area <%= c.OSPF_area %> interface <%= c.interface_identifier %>.<%= c.white_sub_interface_id %> authentication md5 <%= c.key_id %> key juniper123 start-time 2013-01-01.00:00:00;

set protocols ldp interface <%= c.interface_identifier %>.<%= c.white_sub_interface_id %>
set protocols mpls interface <%= c.interface_identifier %>.<%= c.white_sub_interface_id %>
set protocols rsvp interface <%= c.interface_identifier %>.<%= c.white_sub_interface_id %> bandwidth 12m
set protocols pim interface <%= c.interface_identifier %>.<%= c.white_sub_interface_id %> mode sparse
set protocols pim interface <%= c.interface_identifier %>.<%= c.white_sub_interface_id %> version 2

set routing-instances <%= c.red|blue %> interface <%= c.interface_identifier %>.<%= c.coloured_sub_interface_id %> 
set routing-instances <%= c.red|blue %> protocols ospf area <%= c.OSPF_area %> interface <%= c.interface_identifier %>.<%= c.coloured_sub_interface_id %> metric <%= c.ospf_metric %>
set routing-instances <%= c.red|blue %> protocols ospf area <%= c.OSPF_area %> interface <%= c.interface_identifier %>.<%= c.coloured_sub_interface_id %> hello-interval <%= c.OSPF_hello_timer %>
set routing-instances <%= c.red|blue %> protocols ospf area <%= c.OSPF_area %> interface <%= c.interface_identifier %>.<%= c.coloured_sub_interface_id %> dead-interval <%= c.OSPF_dead_timer %>
set routing-instances <%= c.red|blue %> protocols ospf area <%= c.OSPF_area %> interface <%= c.interface_identifier %>.<%= c.coloured_sub_interface_id %> ldp-synchronization   

set routing-instances <%= c.red|blue %> protocols ospf area <%= c.OSPF_area %> interface <%= c.interface_identifier %>.<%= c.coloured_sub_interface_id %> authentication md5 <%= c.key_id %> key juniper123 start-time 2013-01-01.00:00:00;

set protocols ldp interface <%= c.interface_identifier %>.<%= c.coloured_sub_interface_id %>

set protocols pim interface <%= c.interface_identifier %>.<%= c.coloured_sub_interface_id %> mode  sparse 
set protocols pim interface <%= c.interface_identifier %>.<%= c.coloured_sub_interface_id %> version 2 

