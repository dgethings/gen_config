interfaces {
	$physical_interface$  {
        unit $logical_interface$ {
            description $subinterface_description$;
            vlan-id $vlan$;
            family inet {
                mtu 1500;
                address $ip_address$/30; 
				filter {
					input $customer_name$-IN-$physical_interface$;                     
					output $customer_name$-OUT-$physical_interface$;
				}
            }
        }
	}
    interface-set interface_set_$Customer_name$_$Physical_interface$ {
        interface $physical_interface$ {
            unit $logical_interface$;
        }
	}
}
routing-instances {
	$vrf_name$ {
		interface $physical_interface$.$logical_interface$;
		routing-options {
			static {
				route $CE_int_IP_address$/32 {
					next-hop $CE_int_IP_address$;
					community 12641:6100;
				}
				route $CE_mgmt_IP_address$/32 {
					next-hop $CE_int_IP_address$;
				}
				route $cpe_lan_ip_addr$/32 {
					next-hop $CE_IP_address$;
				}
				route $customer_prefix$/$mask$ {
					next-hop $CE_IP_address$;
				}
			}
		}
	}
}